<%- include("partials/header"); -%>

<h1>Compose</h1>
<form action="/compose" method="post">
  <div class="form-group">
    <label>Title</label>
    <input class="form-control" type="text" name="articleTitle">
  </div>

  <div class="form-group">
    <label>Post</label>
    <textarea class="form-control" name="articlePost" cols="30" rows="5"></textarea>
  </div>

  <button class="btn btn-sl btn-primary" type="submit">Publish</button>
</form>

<%- include("partials/footer"); -%>